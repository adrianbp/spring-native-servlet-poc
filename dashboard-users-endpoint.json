{
  "dashboard": {
    "id": null,
    "title": "/api/users Endpoint Monitoring - Native vs JAR",
    "tags": ["endpoint", "performance", "users"],
    "style": "dark",
    "timezone": "browser",
    "time": {
      "from": "now-30m",
      "to": "now"
    },
    "refresh": "5s",
    "schemaVersion": 30,
    "version": 0,
    "panels": [
      {
        "id": 1,
        "title": "üö¶ TPS - GET /api/users",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(http_server_requests_seconds_count{method=\"GET\", uri=\"/api/users\", job=\"spring-native-app\"}[5m])",
            "legendFormat": "Native TPS",
            "refId": "A"
          },
          {
            "expr": "rate(http_server_requests_seconds_count{method=\"GET\", uri=\"/api/users\", job=\"spring-jar-app\"}[5m])",
            "legendFormat": "JAR TPS",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineWidth": 2,
              "fillOpacity": 0.2,
              "pointSize": 5,
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {"group": "A", "mode": "none"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {"type": "linear"},
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "off"}
            },
            "min": 0
          }
        },
        "options": {
          "tooltip": {"mode": "single", "sort": "none"},
          "legend": {"displayMode": "list", "placement": "bottom"},
          "displayMode": "list"
        }
      },
      {
